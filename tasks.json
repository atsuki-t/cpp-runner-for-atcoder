{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "type": "shell",
      "command": "/usr/bin/g++",
      "args": [
        "-std=gnu++20",
        "-Wall",
        "-Wextra",
        "-O2",
        "-DONLINE_JUDGE",
        "-I${workspaceFolder}/ac-library",
        "-o",
        "'${workspaceFolder}/program'",
        "'${file}'"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
